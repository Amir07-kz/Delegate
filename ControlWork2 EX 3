using System;
using System.Collections;
using System.Collections.Generic;
using System.Linq;

namespace CONTROL_WORK_2
{
    public class Point
    {

        double x, y;

        public Point(double newX, double newY)
        {
            x = newX;
            y = newY;
        }
        private bool ChekTop(Point p1, Point p2,Point p3, Point p4)
        {
            if (p2.x < p1.x)
            {
                Point top = p1;
                p1 = p2;
                p2 = top;
            }

            if (p4.x < p3.x)
            {
                Point top = p3;
                p3 = p4;
                p4 = top;
            }
            if (p2.x < p3.x)
            {
                Console.WriteLine("Пересечений нет");
                return false;
            }
        }
    }
}
